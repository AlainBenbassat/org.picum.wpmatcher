<?xml version="1.0"?>
<extension key="org.picum.wpmatcher" type="module">
  <file>wpmatcher</file>
  <name>WP Matcher</name>
  <description>Screen to easily assign Wordpress users and CiviCRM contacts</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-11-13</releaseDate>
  <version>v1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Wpmatcher</namespace>
    <format>22.10.0</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
